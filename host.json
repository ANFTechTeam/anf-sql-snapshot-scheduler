{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "extensions": {
    "cosmosDb": {},
    "durableTask": {},
    "eventHubs": {},
    "http": {
      "routePrefix": "api"
    },
    "queues": {},
    "sendGrid": {},
    "serviceBus": {}
  },
  "managedDependency": {
    "enabled": true
  },
  "logging": {
    "logLevel": {
      "Function.NetAppSQL": "Information",
      "Function.StartProcess": "Information",
      "Function.checkInstall": "Information",
      "Function.StartVM": "Information",
      "Function.Pre-Snapshot": "Information",
      "default": "Information"
    }
  }
}