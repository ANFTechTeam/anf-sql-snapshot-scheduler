{
  "logging": {
    "logLevel": {
      "Function.checkInstall": "Debug",
      "default": "Trace"
    }
  },
  "bindings": [
    {
      "type": "queueTrigger",
      "connection": "Storage",
      "dataType": "string",
      "direction": "in",
      "name": "Request",
      "queueName": "checkInstall"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "installation",
      "queueName": "installation",
      "connection": "Storage"
    },
    
    {
      "type": "queue",
      "direction": "out",
      "name": "installed",
      "queueName": "installed",
      "connection": "Storage"
    }
  ]
}