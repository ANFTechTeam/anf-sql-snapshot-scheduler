{
  "logging": {
    "logLevel": {
      "Function.checkInstall": "Debug",
      "default": "Trace"
    }
  },
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [
        "post",
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    }
  ]
}